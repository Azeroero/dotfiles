if [[ -d ./.svn ]] {
  echo "[\033[0;36mSVN\033[00m]"
} else {
  branch=`git branch --no-color 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/'`
  if [[ `echo "$branch"|wc -c` -gt "1" ]] {
    if [[ `git status 2>/dev/null |wc -l` -gt "2" ]] {
      notice='*'
    } else {
      notice=''
    }
    echo "[\033[0;32m$branch\033[1;31m$notice\033[00m]"
  }
}

